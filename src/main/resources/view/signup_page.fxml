<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.SignupPageController">
    <children>
        <AnchorPane layoutX="719.0" layoutY="-10.0" prefHeight="841.0" prefWidth="569.0" style="-fx-background-color: #ffffff;">
            <children>
                <ImageView fitHeight="113.0" fitWidth="216.0" layoutX="177.0" layoutY="40.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/7733951%20%5BRecovered%5D%202.png" />
                    </image>
                </ImageView>
                <Text fill="#263238" layoutX="80.0" layoutY="247.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Signup">
                    <font>
                        <Font name="Concert One" size="42.0" />
                    </font>
                </Text>
                <Text fill="#6a7b83" layoutX="80.0" layoutY="292.0" strokeType="OUTSIDE" strokeWidth="0.0" text="please enter your details below to signup!">
                    <font>
                        <Font name="Contrail One" size="18.0" />
                    </font>
                </Text>
                <TextField fx:id="userName" layoutX="82.0" layoutY="367.0" prefHeight="61.0" prefWidth="353.0" promptText="     username" style="-fx-background-radius: 15; -fx-background-color: #F3F3F3;">
                    <font>
                        <Font name="Contrail One" size="17.0" />
                    </font>
                </TextField>
                <Button fx:id="loginButton" layoutX="114.0" layoutY="678.0" mnemonicParsing="false" onAction="#signup" prefHeight="61.0" prefWidth="289.0" style="-fx-background-radius: 17; -fx-background-color: #65386D;" text="Signup" textFill="#e0e0e0">
                    <font>
                        <Font name="Contrail One" size="25.0" />
                    </font>
                </Button>
            <PasswordField fx:id="password" layoutX="80.0" layoutY="465.0" prefHeight="61.0" prefWidth="353.0" promptText="    password" style="-fx-background-radius: 15; -fx-background-color: #F3F3F3;">
               <font>
                  <Font size="17.0" />
               </font>
            </PasswordField>
            <TextField fx:id="userId" layoutX="82.0" layoutY="568.0" prefHeight="61.0" prefWidth="353.0" promptText="     user_id" style="-fx-background-radius: 15; -fx-background-color: #F3F3F3;">
               <font>
                  <Font name="Contrail One" size="17.0" />
               </font>
            </TextField>
            </children>
        </AnchorPane>
      <AnchorPane layoutX="-11.0" layoutY="-12.0" prefHeight="845.0" prefWidth="703.0" style="-fx-background-color: #ffffff;">
         <children>
            <Button fx:id="backToLogin" layoutX="117.0" layoutY="748.0" mnemonicParsing="false" onAction="#backToLoginButton" style="-fx-background-color: transparent;" text="Back to login" textFill="#6a7b83" underline="true">
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Button>
            <ImageView fitHeight="683.0" fitWidth="718.0" layoutX="41.0" layoutY="81.0">
               <image>
                  <Image url="@../img/Mesa%20de%20trabajo%202.png" />
